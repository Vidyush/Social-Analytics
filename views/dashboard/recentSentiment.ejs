 <%- include('../_includes/head') %>
  
   
 <div class="chart-area">
    <div class="float-left" id="chart" style="padding-left:250px; width:500px; height:500px; margin: 0 auto;"></div>   
    <div class="content-area2">
            <div class="dash-counter">
                    <ul style="text-align:center">
         
                            <li style="text-align:center"><img width="<%=(typeof(feedback1) !== undefined)? parseFloat(feedback1) : undefined  %>%" src="/images/Positive-face.png" class="img-fluid" alt="" center><span class="caption">Positive <%= (typeof(feedback1) !== undefined)? parseFloat(feedback1) : undefined  %></span></li>	</ul>				
                            <ul style="text-align:center"><li style="text-align:center"><img width="<%=(typeof(feedback2) !== undefined)? parseFloat(feedback2) : undefined  %>%" src="/images/Negative-face.png" class="img-fluid" alt=""><span class="caption">Negative <%=(typeof(feedback2) !== undefined)? parseFloat(feedback2) : undefined  %></span></li></ul>					
                            <ul style="text-align:center"><li style="text-align:center"><img width="<%=(typeof(feedback3) !== undefined)? parseFloat(feedback3) : undefined  %>%" src="/images/neutral-face.png" class="img-fluid" alt=""></span></span><span class="caption">Neutral <%=(typeof(feedback3) !== undefined)? parseFloat(feedback3) : undefined  %></span></li>				
                                        
                    </ul>
            </div>
        </div> 
</div>


  

 <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
 <script type="text/javascript">
   google.charts.load('current', {'packages':['corechart']});
   google.charts.setOnLoadCallback(drawChart);

   function drawChart() {

     var data = google.visualization.arrayToDataTable([
       ['Sentiment', 'Value'],
       ['Positive',   <%= (typeof(feedback1) !== undefined)? parseFloat(feedback1) : undefined  %>],
       ['Negative', <%= (typeof(feedback2) !== undefined)? parseFloat(feedback2) : undefined  %>],
       ['Neutral', <%= (typeof(feedback3) !== undefined)? parseFloat(feedback3) : undefined  %>]
      
     ]);

     var options = {
       title: 'Sentiment',
       is3D:true
     };

     var chart = new google.visualization.PieChart(document.getElementById('chart'));

     chart.draw(data, options);
   }
 </script>

